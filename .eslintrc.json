{
    "extends": [
        "react-app",
        "airbnb"
    ],
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-mixed-spaces-and-tabs": "warn",
        "no-trailing-spaces": "warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "max-len": "off",
        "comma-dangle": "off",
        "jsx-a11y/href-no-hash": "off",
        "react/forbid-prop-types": "off"
    }
}