{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-mixed-spaces-and-tabs": "warn",
        "no-trailing-spaces": "warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    }
}